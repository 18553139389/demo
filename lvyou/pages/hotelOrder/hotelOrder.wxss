.contain {
  width: 100%;
  height: 100vh;
  border-top: 1px solid #eee;
  background: #f8f8f8;
  /* overflow: hidden; */
}

.tab {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  position: fixed;
  top: 160rpx;
  left: 0;
  z-index: 9999;
}

.tab-item {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 14px;
  color: #333;
  box-sizing: border-box;
  position: relative;
}

.active {
  border-bottom: 2px solid #be7e50;
}

.list {
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.list-item {
  width: 100%;
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 4px;
  display: flex;
  flex-direction: column;
  font-size: 12px;
  color: #999;
  margin-bottom: 30rpx;
}

.top {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 16rpx;
}

.top-title {
  font-size: 14px;
  color: #333;
}

.price {
  font-size: 18px;
  color: red;
}

.state {
  color: #2A93DD;
}

scroll-view {
  position: absolute;
  top: 250rpx;
  left: 0;
}

.no {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #999;
}

.no>image {
  width: 300rpx;
  height: 300rpx;
}

.icon {
  width: 24rpx;
  height: 24rpx;
  padding: 2px;
  line-height: 24rpx;
  border-radius: 50%;
  background: red;
  color: #fff;
  font-size: 10px;
  text-align: center;
  position: absolute;
  top: 14rpx;
  right: -16rpx;
  z-index: 999;
}

.tit {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333;
  margin-top: 60rpx;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
  background: #f8f8f8;
}

.back1 {
  width: 20rpx;
  height: 40rpx;
  margin-left: 30rpx;
}

.bgs {
  background: #ededed !important;
}