.contain {
  width: 100%;
  height: 100%;
  border-top: 1px solid #eee;
}

.top {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.top-hotel {
  width: 100%;
  display: flex;
  padding: 18rpx 30rpx;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
}

.titles {
  font-size: 14px;
  color: #333;
}

.details {
  height: 50rpx;
  display: flex;
  align-items: center;
}

.details>view {
  font-size: 12px;
  color: #2A93DD;
  margin-right: 10rpx;
}

.details>image {
  width: 24rpx;
  height: 24rpx;
}

.address {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 12px;
  color: #666;
}

.tab {
  width: 100%;
  height: 90rpx;
  background: #F4FBFF;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin-top: 20rpx;
}

.tab-item {
  height: 90rpx;
  display: flex;
  align-items: center;
  font-size: 14px;
  color: #333;
}

.tab-item>image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.line {
  width: 100%;
  height: 10px;
  background: #f8f8f8;
}

.sku {
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 14px;
  color: #333;
}

.sku>view {
  width: 100%;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
}

.sku-item {
  width: 100%;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.all {
  width: 280rpx;
  font-size: 14px;
  color: #333;
}

.common {
  font-size: 14px;
  color: #666;
}

.commons {
  font-size: 14px;
  color: #FF6B51;
}

.detail {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.produce {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.produce-title {
  width: 100%;
  font-size: 15px;
  color: #333;
  font-weight: 600;
  position: relative;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.produce-title::after {
  content: '';
  width: 2px;
  height: 40rpx;
  background: #BE7E50;
  border-radius: 4px;
  position: absolute;
  left: 0;
  top: 0;
}
