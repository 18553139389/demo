.contain {
  width: 100%;
  height: 100vh;
  border-top: 1px solid #eee;
  background: #f8f8f8;
  overflow: hidden;
}

.s1 {
  background: #fff;
  height: 140rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.list {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
}

.list-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 20rpx;
}

.time {
  font-size: 16px;
  color: #8a8a8a;
  font-weight: 700;
  margin-bottom: 10rpx;
}

.state {
  font-size: 12px;
  color: #8a8a8a;
}

.s2 {
  width: 100%;
  position: absolute;
  top: 140rpx;
  left: 0;
  padding: 30rpx;
  box-sizing: border-box;
}

.coupon {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 4px;
  background: #fff;
  display: flex;
  margin-bottom: 30rpx;
}

.coupon>image {
  width: 285rpx;
  height: 200rpx;
  border-radius: 4px;
}

.coupon-item {
  width: calc(100% - 285rpx);
  height: 200rpx;
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.titles {
  width: 100%;
  font-size: 14px;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.price {
  font-size: 12px;
  color: #666;
}

.time {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.date1 {
  padding: 2px 16px;
  font-size: 12px;
  color: #FF440D;
  background: #F8FBFC;
  border-radius: 4px;
  font-weight: normal;
}

.date2 {
  padding: 4rpx 10rpx;
  background: #FF440D;
  border-radius: 4px;
  font-size: 14px;
  color: #fff;
  font-weight: normal;
}

.num {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 14px;
  color: #FF4B4B;
}

.line1 {
  width: 240rpx;
  height: 10px;
  border-radius: 16px;
  background: #FBE1CF;
  border: 1px solid #BE7E50;
  position: relative;
  overflow: hidden;
}

.line2 {
  /* width: 50%; */
  height: 10px;
  background: #BE7E50;
  border-radius: 16px;
  position: absolute;
  top: 0;
  left: 0;
}

.date3 {
  padding: 4rpx 10rpx;
  background: #ddd;
  border-radius: 4px;
  font-size: 14px;
  color: #fff;
  font-weight: normal;
}

.nos {
  background: #eee !important;
}

.no {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  color: #999;
}

.no>image {
  width: 300rpx;
  height: 300rpx;
}