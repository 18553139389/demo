/* pages/qupiaoren/qupiaoren.wxss */

.qp_2 {
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(17, 17, 17, 1);
}

.qp_3 {
  font-size: 28rpx;
  color: rgba(102, 102, 102, 1);
}

.radioBox {
  font-size: 28rpx;
  color: rgba(102, 102, 102, 1);
}

.qp_4 {
  height: 80rpx;
  border-bottom: 1px solid #f8f8f8;
}

.no {
  width: 100%;
  height: 180rpx;
  line-height: 180rpx;
  font-size: 14px;
  color: #999;
  text-align: center;
}

.van-swipe-cell__left,
.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}

.wrapper {
  width: 100%;
  display: flex;
  flex-direction: column;
  position: relative;
}

.users {
  width: 100%;
  height: 40px;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.list {
  width: 130%;
  height: 40px;
  display: flex;
  align-items: center;
  position: absolute;
  top: 0;
  border-bottom: 1px solid #eee;
}

.alls {
  font-size: 30rpx;
  color: #333;
}

.all {
  font-size: 12px;
  color: #333;
}

.user {
  margin-left: 4%;
  width: 15%;
}

.phone {
  width: 32%;
}

.card {
  width: 48%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.card>image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
}

.del {
  width: 15%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background: red;
  color: #fff;
}

.editor {
  width: 15%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  background: #07c160;
  color: #fff;
}

.add {
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background: #BE7E50;
  border-radius: 20px;
  margin: 50rpx auto 0;
}

