.contain {
  width: 100%;
  height: 100vh;
  position: relative;
}

.bg {
  position: absolute;
  width: 100%;
  height: 100%;
}

.content {
  position: absolute;
  top: 160rpx;
  right: 30rpx;
  padding: 2px 12px;
  background: rgba(0,0,0,0.5);
  font-size: 14px;
  color: #fff;
  border-radius: 12px;
}
