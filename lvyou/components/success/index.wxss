/* components/success/index.wxss */

.su_img {
  width: 119rpx;
  height: 119rpx;
  margin-top: 150rpx;
}

.su_title {
  text-align: center;
  padding: 40rpx 0 10rpx;
}

.su_title text {
  font-size: 30rpx;
  font-weight: bold;
  color: #444;
}

.su_btn {
  width: 362rpx;
  height: 88rpx;
  background: rgba(0, 74, 86, 1);
  border-radius: 12rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 88rpx;
  margin-top: 40rpx;
}

.su_date {
  text-align: center;
  padding-top: 24rpx;
  padding-bottom: 30rpx;
}

.su_date {
  font-size: 26rpx;
  color: rgba(68, 68, 68, 1);
}
