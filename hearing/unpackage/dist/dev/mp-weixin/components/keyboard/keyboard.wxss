}
.keyborad .key_main .main_keyboard image {
  width: 48rpx;
  height: 48rpx;
}
.keyborad .key_main .main_keyboard .key_num,
.keyborad .key_main .main_keyboard .key_null,
.keyborad .key_main .main_keyboard .key_del,
