.keyborad {
  width: 100vw;
  height: 100vh;
  position: fixed;
  bottom: 0px;
  top: 0px;
  left: 0px;
  right: 0px;
  z-index: 100;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.keyborad .key_main {
  width: 100vw;
  height: 900rpx;
  background: whitesmoke;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.keyborad .key_main .main_title {
  font-size: 30rpx;
  color: #000000;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  letter-spacing: 2rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0px 20rpx;
  border-bottom: 2rpx solid #e1e1e1;
}
.keyborad .key_main .main_title image {
  width: 48rpx;
  height: 48rpx;
}
.keyborad .key_main .main_title text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: -48rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.keyborad .key_main .main_content {
  width: 100%;
  box-sizing: border-box;
  padding: 0px 30rpx;
  margin-top: 40rpx;
}
.keyborad .key_main .main_content .content_num {
  width: 100%;
  height: 100rpx;
  border: 2rpx solid #DBDBDB;
  border-radius: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.keyborad .key_main .main_content .content_num .content_item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100%;
  border-right: 2rpx solid #DBDBDB;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.keyborad .key_main .main_content .content_num .content_item:last-child {
  border-right: none;
}
.keyborad .key_main .main_content .main_forget {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  font-size: 28rpx;
  color: #007AFF;
  margin-top: 40rpx;
}
