.uls {
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  overflow: hidden;
  color: #666;
}

.uls .lis {
  border-bottom: 1rpx solid #f0f0f0;
  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
}

.uls .lis .flag {
  position: absolute;
  top: 0;
  right: 0;
  color: #999;
  font-size: 40rpx;
}
