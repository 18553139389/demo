.activeClass {
  display: none;
}

.header {
  text-align: center;
}

.header .imgdv {
  width: 320rpx;
  height: 225rpx;
  margin: 40rpx auto 0;
}

.header .imgdv image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.header .txt {
  font-size: 30rpx;
  display: inline-block;
  margin-top: 40rpx;
}

.btnOrder {
  width: 448rpx;
  height: 70rpx;
  background-color: #1197f6;
  border-radius: 70rpx;
  margin: 50rpx auto 0;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  line-height: 70rpx;
  box-shadow: #c6e6fd 1rpx 10rpx 20rpx;
}

.carousel {
  width: 680rpx;
  height: 202rpx;
  background-color: #1197f6;
  margin: 0 auto;
  margin-top: 160rpx;
  box-shadow: #e6eef3 1rpx 1rpx 10rpx 10rpx;
}

.carousel .swiper {
  width: 680rpx;
  height: 202rpx;
}

.carousel .swiper image {
  height: 100%;
  width: 100%;
}

.adbj {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 20;
  background-color: rgba(0, 0, 0, 0.5);
}

.adbj .adimg {
  width: 575rpx;
  height: 760rpx;
  margin: 160rpx auto 0;
  box-shadow: #6e6e6e 1rpx 1rpx 20rpx 10rpx;
}

.adbj .adimg image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.adbj .yuan {
  width: 40rpx;
  height: 40rpx;
  border: 1rpx solid #fff;
  border-radius: 50%;
  text-align: center;
  line-height: 35rpx;
  color: #fff;
  position: absolute;
  bottom: 16%;
  left: 50%;
  transform: translateX(-50%);
}
.zzc{width: 100%;height: 100%;position: absolute;z-index: 99;background: rgba(0, 0, 0, 0.5);top:0px;left: 0px;}
.tank{width: 400rpx;height: 284rpx;position: absolute;top:50%;left: 50%;margin-left:-200rpx;margin-top:-142rpx;border-radius: 10px;z-index: 999;background: #fff;}
.tank_tit {width: 100%;font-size:14px;text-align: center;line-height: 80rpx;border-bottom:1px solid #e9e9e9;}
.tank_cen {width: 100%;text-align: center;font-size:13px;line-height: 48rpx;padding-top:10rpx;}
.tank navigator {width: 50%;height: 80rpx;border-top:1px solid #e9e9e9;border-right:1px solid #e9e9e9;float: left;box-sizing:border-box;font-size:14px;text-align: center;line-height: 80rpx;background: #fff;}
.tk_qx {width: 50%;height: 80rpx;border-top:1px solid #e9e9e9;font-size:14px;color: #999;float: left;text-align: center;line-height: 80rpx;}
