button::after {
  padding: 0;
  margin:0 ;
  border:none;position: static;
padding-left:0px ;padding-right:0px;
}
/* .form_item{float: left;width: 50%;  background-color: #fff;}*/
.btns {  
  display: inline-block;
  text-align: center;
    background-color: #ef0000;font-size:15px;line-height: 100rpx;
  color: #fff;} 
.cont {
  box-sizing: border-box;
  width: 90%;
  height: 300rpx;
  border: 1px solid #f0f0f0;
  font-size: 30rpx;
  margin: 20rpx auto 0;
  padding-left: 20rpx;
  padding-right: 20rpx;
  box-shadow: 0 0 20rpx 4rpx #f0f0f0;
}

.cont view {
  margin-top: 17rpx;
}

.cont view text {
  color: #666;
}

.time {
  box-sizing: border-box;
  width: 90%;
  height: 100%;
  border: 1px solid #f0f0f0;
  box-shadow: 0 0 20rpx 4rpx #f0f0f0;
  margin: 20rpx auto 0;
  padding: 20rpx;
  font-size: 30rpx;
}

.time .title {
  float: left;
  width: 15%;
  /* height: 100%; */
}

.time .content {
  float: left;
  width: 80%;
  height: 100%;
}

/* 修改单选按钮样式-选中 */

radio .wx-radio-input {
  border-radius: 50%;
  width: 27rpx;
  height: 27rpx;
}

radio .wx-radio-input.wx-radio-input-checked {
  /* border: 1px solid #1197f6; *//* background-color: #1197f6; */
}

radio .wx-radio-input.wx-radio-input-checked::before {
  border-radius: 50%;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
  background-color: #1197f6;
}

#timeDv {
  overflow: hidden;
  margin-bottom: 25rpx;
}

#timeDv .time-title {
  font-size: 28rpx;
}

#timeDv .heng {
  width: 50%;
  float: left;
  margin-top: 20rpx;
  color: #666;
  font-size: 26rpx;
}

#timeDv .heng .radcont {
  float: left;
}

#timeDv .heng .txt1 {
  float: left;
  margin-top: 4rpx;
}

.integral {
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #f0f0f0;
  font-size: 30rpx;
  margin: 10rpx auto 0;
}

.integral .left {
  float: left;
  color: #f18c16;
}

.integral .right {
  float: right;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
  width: 27rpx;
  height: 27rpx;
}

checkbox .wx-radio-input.wx-radio-input-checked {
  /* border: 1px solid #1197f6; *//* background-color: #1197f6; */
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  border-radius: 50%;
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
  background-color: #1197f6;
}

.integral .right .textinfo {
  float: right;
  margin-top: 2rpx;
}

.integral2 {
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #f0f0f0;
  font-size: 30rpx;
  margin: 0 auto;
  position: relative;
}

.integral2 .left {
  float: left;
}

.integral2 .right {
  float: right;
  color: #ef0000;
}

.integral2 .pst {
  position: absolute;
  bottom: -70rpx;
  right: 0;
  font-size: 28rpx;
  color: #999;
}

.footer {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0rpx;
  box-shadow: #f0f0f0 1rpx 0 20rpx 5rpx;
  font-size: 30rpx;
  font-weight: 700;
}

.footer .left {
  width: 50%;
float: left;
  text-align: center;line-height: 100rpx;
}

.footer .right {
  width: 50%;
  float: left;
  text-align: center;
  background-color: #ef0000;
  color: #fff;
}
