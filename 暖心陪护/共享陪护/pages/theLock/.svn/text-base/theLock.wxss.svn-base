/* pages/theLock/theLock.wxss */
.activeClass {
  display: none;
}

.header {
  text-align: center;
}

.header .imgdv {
  width: 320rpx;
  height: 225rpx;
  margin: 40rpx auto 0;
}

.header .imgdv image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.header .txt {
  font-size: 30rpx;
  display: inline-block;
  margin-top: 40rpx;
}

.btnOrder {
  width: 448rpx;
  height: 70rpx;
  background-color: #1197f6;
  border-radius: 70rpx;
  margin: 50rpx auto 0;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  line-height: 70rpx;
  box-shadow: #c6e6fd 1rpx 10rpx 20rpx;
}

