.box_lis {
  margin-top: 20rpx;
  font-size: 30rpx;
}

.box_lis .lis {
  height: 80rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.box_lis .lis .name {
  float: left;
  margin-top: 21rpx;
  margin-left: 30rpx;
}

.box_lis .lis  .number {
  float: right;
  margin-top: 21rpx;
  margin-right: 30rpx;
}

.box_lis .lis .box_dv {
  float: right;
  width: 350rpx;
  height: 64rpx;
  border: 1rpx solid #f0f0f0;
  margin-top: 7rpx;
  margin-right: 30rpx;
  line-height: 64rpx;
  padding-right: 55rpx;
  box-sizing: border-box;
  padding-left: 15rpx;
}

.box_lis .lis .box_dv_flag {
  position: absolute;
  width: 22rpx;
  height: 20rpx;
  top: 20rpx;
  right: 50rpx;
}

.box_lis .lis .box_dv_flag image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.box_lis .lis .box_dv2 {
  float: right;
  width: 287rpx;
  height: 64rpx;
  border: 1rpx solid #f0f0f0;
  margin-top: 7rpx;
  margin-right: 30rpx;
  line-height: 64rpx;
}

.box_lis .lis .info {
  float: right;
  margin-top: 20rpx;
  margin-right: 31rpx;
}

.box_lis .lis .box_dv2 .section {
  width: 100%;
  height: 100%;
  text-align: center;
}

.box_lis .lis .box_dv2 .section .ipt {
  height: 100%;
}

.submitbtn {
  width: 448rpx;
  height: 70rpx;
  background-color: #1197f6;
  border-radius: 70rpx;
  margin: 120rpx auto 0;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  line-height: 70rpx;
}

.boxyuan {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99;
}

.boxyuan .boxbox {
  width: 80%;
  height: 893rpx;
  background-color: #fff;
  margin: 66rpx auto 0;
  border-radius: 10rpx;
  overflow: hidden;
}

.boxyuan .boxbox .title {
  font-size: 32rpx;
  height: 83rpx;
  width: 100%;
  text-align: center;
  line-height: 83rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.boxyuan .boxbox .lis {
  font-size: 28rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 1rpx solid #f0f0f0;
  padding-left: 80rpx;
  position: relative;
}

.boxyuan .boxbox .lis .tux {
  width: 30rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 30rpx;
}

.boxyuan .boxbox .lis .tux image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.boxyuan .boxbox .lis .flagtu {
  position: absolute;
  top: 0rpx;
  right: 20rpx;
  font-size: 38rpx;
  color: #999;
}

.boxyuan .clearX {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #fff;
  color: #fff;
  text-align: center;
  line-height: 38rpx;
  font-size: 40rpx;
  margin: 30rpx auto;
}

.activeClass {
  display: none;
}
