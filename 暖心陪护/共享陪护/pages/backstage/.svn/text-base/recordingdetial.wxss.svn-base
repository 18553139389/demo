.liss {
  height: 246rpx;
  width: 90%;
  margin: 28rpx auto 0;
  box-shadow: #f0f0f0 1rpx 1rpx 30rpx 5rpx;
}

.liss .lis_cont {
  height: 164rpx;
  width: 100%;
  border-bottom: 1rpx solid #f0f0f0;
  font-size: 30rpx;
  position: relative;
}

.liss .lis_cont .lis_title {
  position: absolute;
  top: 35rpx;
  left: 30rpx;
}

.liss .lis_cont .lis_code {
  position: absolute;
  bottom: 20rpx;
  left: 80rpx;
  font-size: 28rpx;
}

.liss .lis_cont .lis_info {
  color: #666;
  position: absolute;
  bottom: 20rpx;
  right: 40rpx;
  font-size: 26rpx;
}

.liss .lis_cont .lis_flag {
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  bottom: 20rpx;
  left: 38rpx;
}

.liss .lis_cont .lis_flag image {
  width: 100%;
  height: 100%;
}

.liss .lis_bottom {
  font-size: 24rpx;
  width: 100%;
  height: 81rpx;
  line-height: 81rpx;
}

.liss .lis_bottom .lis_left {
  float: left;
  color: #999;
  margin-left: 30rpx;
}

.info1 {
  font-size: 30rpx;
  width: 90%;
  margin: 20rpx auto 0;
}

.info2 {
  font-size: 26rpx;
  color: #999;
  width: 90%;
  margin: 15rpx auto 0;
}

.info3 {
  font-size: 26rpx;
  color: #999;
  width: 90%;
  margin: 15rpx auto 0;
}

.dvtp {
  margin-top: 30rpx;
}

 .dvtp_img {
  width: 175rpx;
  height: 175rpx;
  float: left;
  margin-left: 5%;
}

 .dvtp_img image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.dvtp2 {
  margin-top: 30rpx;
}

.dvtp2 .dvtp_img {
  width: 175rpx;
  height: 175rpx;
  float: left;
  margin-left: 5%;
}

.dvtp2 .dvtp_img:nth-child(1) {
  margin-left: 11%;
}

.dvtp2 .dvtp_img image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.hengx {
  width: 90%;
  height: 1rpx;
  background-color: #f0f0f0;
  margin: 0 auto;
  margin-top: 30rpx;
}

.marginDv {
  margin-bottom: 40rpx;
}
