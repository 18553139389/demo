.box_time {
  margin-top: 20rpx;
  font-size: 26rpx;
}

.box_time .box_time_left {
  width: 220rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  border: 1rpx solid #f0f0f0;
  margin-left: 36rpx;
  float: left;
}

.box_time .box_time_jian {
  width: 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  float: left;
  text-align: center;
  margin-left: 30rpx;
}

.box_time .box_time_right {
  width: 220rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  border: 1rpx solid #f0f0f0;
  margin-left: 30rpx;
  float: left;
}

.box_time .box_btn {
  width: 120rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #1197f6;
  color: #fff;
  font-size: 30rpx;
  float: right;
  text-align: center;
  border-radius: 8rpx;
  margin-right: 35rpx;
}

.header {
  width: 90%;
  height: 106rpx;
  box-shadow: #f0f0f0 1rpx 1rpx 20rpx 2rpx;
  margin: 50rpx auto 0;
  font-size: 30rpx;
  line-height: 106rpx;
  position: relative;
}

.header .text {
  margin-left: 80rpx;
}

.header .img {
  width: 30rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 30rpx;
}

.header .img  image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.header .flag {
  position: absolute;
  top: 2rpx;
  right: 40rpx;
  font-size: 40rpx;
  color: #999;
}

.info_dv {
  width: 90%;
  height: 310rpx;
  box-shadow: #f0f0f0 1rpx 1rpx 20rpx 2rpx;
  margin: 40rpx auto 0;
  font-size: 28rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
  color: #666;
}

.info_dv .info_dv_left {
  float: left;
  height: 50rpx;
  width: 40%;
  line-height: 50rpx;
  margin-top: 30rpx;
}

.info_dv .info_dv_right {
  float: left;
  height: 50rpx;
  width: 40%;
  line-height: 50rpx;
  margin-top: 30rpx;
  margin-left: 70rpx;
}

.info_dv .dan1 {
  color: #000;
}

.info_dv .dan2 {
  color: #f00;
}

.info_dv .dan3 {
  color: #f18c16;
}

.boxyuan {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
}

.boxyuan .boxbox {
  width: 80%;
  height: 893rpx;
  background-color: #fff;
  margin: 66rpx auto 0;
  border-radius: 10rpx;
  overflow: hidden;
}

.boxyuan .boxbox .title {
  font-size: 32rpx;
  height: 83rpx;
  width: 100%;
  text-align: center;
  line-height: 83rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.boxyuan .boxbox .lis {
  font-size: 28rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 1rpx solid #f0f0f0;
  padding-left: 80rpx;
  position: relative;
}

.boxyuan .boxbox .lis .tux {
  width: 30rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 30rpx;
}

.boxyuan .boxbox .lis .tux image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.boxyuan .boxbox .lis .flagtu {
  position: absolute;
  top: 0rpx;
  right: 20rpx;
  font-size: 38rpx;
  color: #999;
}

.boxyuan .clearX {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #fff;
  color: #fff;
  text-align: center;
  line-height: 38rpx;
  font-size: 40rpx;
  margin: 30rpx auto;
}

.activeClass {
  display: none;
}
