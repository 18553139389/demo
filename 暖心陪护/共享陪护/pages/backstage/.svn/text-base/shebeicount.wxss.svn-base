page {
  padding-bottom: 150rpx;
}

.header {
  width: 90%;
  height: 106rpx;
  box-shadow: #f0f0f0 1rpx 1rpx 20rpx 2rpx;
  margin: 30rpx auto 0;
  font-size: 30rpx;
  line-height: 106rpx;
  position: relative;
}

.header .text {
  margin-left: 80rpx;
}

.header .img {
  width: 30rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 30rpx;
}

.header .img  image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.header .flag {
  position: absolute;
  top: 2rpx;
  right: 40rpx;
  font-size: 40rpx;
  color: #999;
}

.huibj {
  width: 100%;
  height: 68rpx;
  text-align: center;
  font-size: 28rpx;
  line-height: 68rpx;
  margin-top: 18rpx;
  background-color: #f8f8f8;
  color: #f18c16;
}

.tabar_box {
  width: 100%;
  height: 96rpx;
  border-bottom: 1rpx solid #f0f0f0;
  font-size: 30rpx;
  overflow: hidden;
  white-space: nowrap;
}

.tabar_box .scroll_box {
  width: 100%;
  overflow: hidden;
  height: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.tabar_box .scroll_box  .scroll_dv {
  width: auto;
  height: 92rpx;
  margin-right: 30rpx;
  display: inline-block;
  text-align: center;
  line-height: 92rpx;
}

.activeTabar {
  border-bottom: 4px solid #1197f6;
  color: #1197f6;
}

.listbox {
  width: 90%;
  height: 100%;
  margin: 20rpx auto 0;
  box-shadow: #f0f0f0 1rpx 1rpx 20rpx 2rpx;
}

.listbox .listbox_title {
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid #f0f0f0;
  font-size: 30rpx;
  position: relative;
}

.listbox .listbox_title .list_img {
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-left: 40rpx;
  margin-top: 33rpx;
}

.listbox .listbox_title .list_img  image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.listbox .listbox_title .list_code {
  float: left;
  margin-left: 15rpx;
  margin-top: 32rpx;
}

.listbox .listbox_title .list_txt {
  float: right;
  color: #999;
  margin-top: 30rpx;
  margin-right: 30rpx;
}

.listbox .listbox_title .kong {
  font-size: 24rpx;
  text-align: center;
  position: absolute;
  left: 220rpx;
  top: 30rpx;
  width: 100rpx;
  height: 45rpx;
  line-height: 45rpx;
  background-color: #e7f4fe;
  color: #1197f6;
  border-radius: 8rpx;
}

.listbox .listbox_title .yong {
  font-size: 24rpx;
  text-align: center;
  position: absolute;
  left: 220rpx;
  top: 30rpx;
  width: 100rpx;
  height: 45rpx;
  line-height: 45rpx;
  background-color: #fef3e7;
  color: #f18c16;
  border-radius: 8rpx;
}

.listbox .listbox_cont {
  width: 100%;
  height: auto;
  font-size: 30rpx;
  position: relative;
}

.listbox .listbox_cont .left {
  float: left;
  color: #666;
  margin-left: 35rpx;
}

.listbox .listbox_cont .left1 {
  margin-top: 20rpx;
}

.listbox .listbox_cont .left2 {
  margin-top: 20rpx;
}

.listbox .listbox_cont .right {
  float: left;
  margin-left: 10rpx;width:70%;
}

.listbox .listbox_cont .right1 {
  margin-top: 20rpx;font-size:14px;
}

.listbox .listbox_cont .right2 {
  color: #f00;
  margin-top: 20rpx;
}

.listbox .listbox_cont .right_float {
  position: absolute;
  bottom: 7rpx;
  right: 35rpx;
  color: #999;
}

.listbox .listbox_cont .right_float .red {
  color: #f00;
}

.searchbtn {
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  box-shadow: #f0f0f0 0rpx -6rpx 10rpx 3rpx;
  font-size: 26rpx;
  background-color: #fff;
}

.searchbtn .section {
  height: 72rpx;
  line-height: 72rpx;
  width: 70%;
  border: 1rpx solid #f0f0f0;
  margin-top: 12rpx;
  margin-left: 60rpx;
}

.searchbtn .section .input {
  text-align: center;
  line-height: 72rpx;
  width: 100%;
  height: 100%;
}

.searchbtn .searbtn {
  width: 120rpx;
  height: 74rpx;
  background-color: #1197f6;
  text-align: center;
  line-height: 74rpx;
  border-radius: 8rpx;
  position: absolute;
  top: 12rpx;
  right: 35rpx;
  color: #fff;
}

.boxyuan {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
}

.boxyuan .boxbox {
  width: 80%;
  height: 893rpx;
  background-color: #fff;
  margin: 66rpx auto 0;
  border-radius: 10rpx;
  overflow: hidden;
}

.boxyuan .boxbox .title {
  font-size: 32rpx;
  height: 83rpx;
  width: 100%;
  text-align: center;
  line-height: 83rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.boxyuan .boxbox .lis {
  font-size: 28rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 1rpx solid #f0f0f0;
  padding-left: 80rpx;
  position: relative;
}

.boxyuan .boxbox .lis .tux {
  width: 30rpx;
  height: 40rpx;
  position: absolute;
  top: 12rpx;
  left: 30rpx;
}

.boxyuan .boxbox .lis .tux image {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.boxyuan .boxbox .lis .flagtu {
  position: absolute;
  top: 0rpx;
  right: 20rpx;
  font-size: 38rpx;
  color: #999;
}

.boxyuan .clearX {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1rpx solid #fff;
  color: #fff;
  text-align: center;
  line-height: 38rpx;
  font-size: 40rpx;
  margin: 30rpx auto;
}

.activeClass {
  display: none;
}
