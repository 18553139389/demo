button::after {
  padding: 0;
  margin:0 ;
  border:none;position: static;
padding-left:0px ;padding-right:0px;
}
.header {
  width: 100%;
  height: 130rpx;
  border-bottom: 1rpx solid #f0f0f0;
  border-top: 1rpx solid #f0f0f0;
}

.header .tximage {
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
  /* background-color: #1197f6; */
  float: left;
  margin-top: 29rpx;
  margin-left: 29rpx;
}

.header .tximage image {
  width: 100%;
  height: 100%;
}

.header .text {
  float: left;
  font-size: 30rpx;
  margin-top: 45rpx;
  margin-left: 20rpx;
}

.header .right {
  width: 160rpx;
  height: 60rpx;
  float: right;
  background-color: #1197f6;
  border-radius: 45rpx 0 0 45rpx;
  margin-top: 38rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  line-height: 60rpx;
  box-shadow: #b9def9 0 6rpx 5rpx;
}

.cont .yuan {
  width: 350rpx;
  height: 350rpx;
  border-radius: 50%;
  margin: 0 auto;
  margin-top: 40rpx;
  position: relative;
}
.yuan button{
  width:350rpx;
height:350rpx;

}
.cont .yuan image {
  width: 100%;
  height: 100%;
}

.cont .yuan .text {
  font-size: 28rpx;
  position: absolute;
  bottom: 90rpx;
  left: 120rpx;
  color: #fff;
}

.cont .txtid {
  width: 262rpx;
  height: 10rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  position: relative;
}

.cont .txtid image {
  width: 100%;
  height: 100%;
}

.cont .txtid .text {
  font-size: 28rpx;
  position: absolute;
  top: -15rpx;
  left: 50rpx;
  color: #ffbd0c;
}

.cont .button {
  width: 448rpx;
  height: 75rpx;
  margin: 0 auto;
  background-color: #1197f6;
  margin-top: 80rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 68rpx;
  text-align: center;
  line-height: 75rpx;
  box-shadow: #b9def9 0 6rpx 5rpx;
}

.borderBj {
  width: 100%;
  height: 10rpx;
  background-color: #f5f5f5;
  margin-top: 60rpx;
}

.carousel {
  width: 700rpx;
  height: 202rpx;
  background-color: #1197f6;
  margin: 0 auto;
  margin-top: 55rpx;
}

.carousel .swiper {
  width: 700rpx;
  height: 202rpx;
}

.carousel .swiper image {
  height: 100%;
  width: 100%;
}

.box_login {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
}

.box_login .login_dv {
  width: 570rpx;
  background-color: #fff;
  margin: 280rpx auto 0;
  border-radius: 10rpx;
  font-size: 30rpx;
  overflow: hidden;
}

.box_login .login_dv .title {
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.box_login .login_dv .iptdv {
  height: 99rpx;
  width: 492rpx;
  border-bottom: 1rpx solid #f0f0f0;
  margin: 0 auto;
}

.box_login .login_dv .iptdv .ipt {
  width: 100%;
  height: 100%;
}

.box_login .login_dv .iptdv2 {
  height: 99rpx;
  width: 492rpx;
  border-bottom: 1rpx solid #f0f0f0;
  margin: 0 auto;
  position: relative;
}

.box_login .login_dv .iptdv2 .ipt2 {
  width: 260rpx;
  height: 100%;
}

.box_login .login_dv .iptdv2 .getyan {
  font-size: 26rpx;
  position: absolute;
  top: 18rpx;
  right: 0rpx;
  color: #f18c16;
  width: 175rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-left: 1rpx solid #f0f0f0;
}

.box_login .login_dv .login_btn {
  width: 354rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #1197f6;
  border-radius: 70rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  margin: 40rpx auto;
}

.tablis {
  width: 100%;
  height: 98rpx;
  border-top: 1rpx solid #f0f0f0;
  position: absolute;
  left: 0;
  bottom: 0;background: #fff;z-index: 999;
}

.tablis .lis1 {
  float: left;
  width: 25%;
  height: 100%;
  text-align: center;
  overflow: hidden;
}
.tablis .lis1 button{
  padding: 0;
  margin:0 ;
  border:0;position: static;
}

.tablis .lis1 .lisimg {
  width: 46rpx;
  height: 38rpx;
  margin: 0 auto;
  margin-top: 10rpx;clear: both
}

.tablis .lis1 .lisimg image {
  width: 100%;
  height: 100%;
  display: block;
}

.tablis .lis1 .listxt {
  font-size: 24rpx;
  color: #1a1a1a;
  margin-top: 10rpx;line-height:16px;
}

.activeClass {
  display: none;
}

.mo_big {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1111;

  overflow: hidden;
}
