button::after {
  padding: 0;
  margin:0 ;
  border:none;position: static;
padding-left:0px ;padding-right:0px;
}
.cont {
  box-sizing: border-box;
  width: 90%;
  height: 350rpx;
  margin: 20rpx auto 0;
  border: 1rpx solid #f0f0f0;
  box-shadow: 0 0 20rpx 4rpx #f0f0f0;
  padding: 0 30rpx;
}

.cont text {
  font-size: 30rpx;
  color: #1a1a1a;
}

.cont .txt1 {
  display: block;
  height: 46rpx;
}

.cont .txt2 {
  color: #666;
}

.zzc{width: 100%;height: 100%;position: absolute;z-index: 99;background: rgba(0, 0, 0, 0.5);top:0px;left: 0px;}
.tank{width: 480rpx;height: 284rpx;position: absolute;top:50%;left: 50%;margin-left:-240rpx;margin-top:-142rpx;border-radius: 10px;z-index: 999;background: #fff;}
